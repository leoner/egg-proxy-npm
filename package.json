{
  "name": "egg-proxy-npm",
  "version": "1.0.1",
  "description": "npm 资源代理",
  "eggPlugin": {
    "name": "proxyNpm"
  },
  "keywords": [
    "egg",
    "eggPlugin",
    "egg-plugin",
    "npm",
    "unpkg"
  ],
  "dependencies": {
    "mime-types": "^2.1.35"
  },
  "devDependencies": {
    "antd": "^4.19.5",
    "autod": "^3.1.2",
    "autod-egg": "^1.1.0",
    "egg": "^2.35.0",
    "egg-bin": "^4.19.0",
